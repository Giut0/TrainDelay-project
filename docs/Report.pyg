		mutation updateCheckoutShippingOptions(
		$checkoutId: ID!
		$shippingMethodId: ID!
		) {
			checkoutShippingMethodUpdate(
			checkoutId: $checkoutId
			shippingMethodId: $shippingMethodId
			) {
				errors {
					field
					message
					__typename
				}
				checkout {
					...Checkout
					__typename
				}
				__typename
			}
		}
